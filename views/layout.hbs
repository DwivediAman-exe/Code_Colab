  <body>

		<nav class="navbar navbar-expand-lg navbar-dark bg-dark">
			<a class="navbar-brand" href="#"><i class="fas fa-code-branch"></i> CodeColab</a>
			<button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarTogglerDemo02"
				aria-controls="navbarTogglerDemo02" aria-expanded="false" aria-label="Toggle navigation">
				<span class="navbar-toggler-icon"></span>
			</button>
		
			<div class="collapse navbar-collapse" id="navbarTogglerDemo02">
				<ul class="navbar-nav mr-auto mt-2 mt-lg-0">
					<li class="nav-item py-2">
						<a class="nav-link navlinks" href="/">Home<span class="sr-only">(current)</span></a>
					</li>
					<li class="nav-item py-2">
						<a class="nav-link navlinks" href="/about">About</a>
					</li>
					<li class="nav-item py-2">
						<a class="nav-link navlinks" href="/contact">Contact</a>
					</li>
				</ul>
				<ul class="nav navbar-nav navbar-right">
					{{#if user}}
						<div class="btn-group adminuser">
							<button type="button" class="btn btn-secondary">{{user.name}}</button>
							<button type="button" class="btn btn-info dropdown-toggle dropdown-toggle-split" data-toggle="dropdown"
								aria-haspopup="true" aria-expanded="false">
								<span class="sr-only">Toggle Dropdown</span>
							</button>
							<div class="dropdown-menu">
								<a class="dropdown-item" href="/logout">Logout</a>
							</div>
						</div>
					{{else}}
						<li class="nav-item py-2 login">
							<a class="nav-link" href="/login">Login</a>
						</li>
						<li class="nav-item py-2 register">
							<a class="nav-link" href="/register">Register</a>
						</li>
					{{/if}}
				</ul>
			</div>
		</nav>

    {{{body}}}


		<footer >
